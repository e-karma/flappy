{{#page-header}}
  <h1 class="pull-left">Choose a Layout</h1>
{{/page-header}}
{{#page-body}}
  {{! model is a theme object }}
<div class="container">
  <div class="box">
    <div class="box-content">
      <div class="gallery">
        <ul class="list-unstyled list-inline">
          searchthistexttofinderror
          {{#each model as |page|}}
            {{#page-theme page=page}}
            
              <li>
                {{#link-to 'editor' page.prototypePage class="link-text-color"}}
                  <div class="picture">
                    <div class="tags">

                    </div>
                    <div class="actions">
                      <div class="pull-left">
                        <small>
                          <i class="icon-user"></i>
                          {{page.displayName}}
                        </small>
                      </div>
                      <div class="pull-right">
                      </div>
                    </div>
                    <img src="{{page.thumbnailUrl}}" class="image-resize" />
                  </div>
                {{/link-to}}
              </li>

            {{/page-theme}}
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
{{/page-body}}
